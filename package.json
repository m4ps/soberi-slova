{
  "name": "endless-word-grid",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:proxy": "node scripts/run-sdk-dev-proxy.mjs dev",
    "dev:proxy:prod": "node scripts/run-sdk-dev-proxy.mjs prod",
    "build": "npm run typecheck && vite build",
    "clean:init": "rm -rf dist output progress.md && find . -name .DS_Store -delete",
    "clean:data": "npm run clean:init && rm -f data/*.tmp.csv data/*.tmp.json data/*.dump.csv data/*.dump.json data/*.draft.csv data/*.draft.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,css}\" \"tests/**/*.ts\" \"*.{json,html,css,mjs}\" \".github/workflows/*.yml\"",
    "format:check": "prettier --check \"src/**/*.{ts,css}\" \"tests/**/*.ts\" \"*.{json,html,css,mjs}\" \".github/workflows/*.yml\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "ci:baseline": "npm run typecheck && npm run test && npm run lint && npm run format:check && npm run build",
    "test:e2e:test-012": "node tests/e2e/test-012-target-word-submit.mjs"
  },
  "dependencies": {
    "pixi.js": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^24.10.13",
    "@yandex-games/sdk-dev-proxy": "^0.0.2",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
